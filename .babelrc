{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    "syntax-dynamic-import",
    "@babel/plugin-proposal-class-properties",
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ], // `style: true` for less,
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "components": "./components/"
        }
      }
    ]
  ]
}
